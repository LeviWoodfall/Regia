{
  "identifier": "default",
  "description": "Default permissions for Regia desktop app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-execute",
    "http:default",
    "http:allow-fetch",
    "http:allow-fetch-cancel",
    "http:allow-fetch-read-body",
    "http:allow-fetch-send",
    "process:default",
    "process:allow-exit",
    "process:allow-restart",
    "os:default",
    "os:allow-arch",
    "os:allow-hostname",
    "os:allow-os-type",
    "os:allow-platform",
    "notification:default",
    "notification:allow-is-permission-granted",
    "notification:allow-request-permission",
    "notification:allow-notify"
  ]
}
